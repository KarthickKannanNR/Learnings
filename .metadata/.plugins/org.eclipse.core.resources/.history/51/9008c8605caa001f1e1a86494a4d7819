package com.java.concepts;

public class Loops {

	public static void loopConcepts() {
		int var =0;
		Object o;
		for(;;var+=999999999) {
			o = new Object();
			System.out.println(var);
		}
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		loopConcepts();
	}

}
