package com.java.syncronizedExample;

class Cake extends Thread{
	
	public void run() {
		System.out.println("Baking cake "+Cake.currentThread().getId());
	}
}

public class ThreadBaking {

	public static void main(String[] args) {
		
		for(int i=1;i<=4;i++) {
			Cake cake = new Cake();
			cake.start();
		}
	}
}
