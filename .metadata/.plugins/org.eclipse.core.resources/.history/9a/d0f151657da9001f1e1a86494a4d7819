package com.java.Multithreading;

public class InterupptDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Thread t = new Thread (()->{
			try {
				Thread.sleep(5000);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
		});
	}

}
